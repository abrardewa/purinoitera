{
  "expo": {
    "name": "Covid",
    "slug": "Covid",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/screen.png",
      "resizeMode": "cover"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#FFFFFF",
        "resizeMode": "contain"
      },
      "package": "com.mostafabenlagha.Covid"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
